{
  "format": "page",
  "id": "project",
  "label": "project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Name",
          "placeholder": "Project name",
          "allowTargetBlank": false,
          "single": "heading3"
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "poster": {
        "type": "Image",
        "config": {
          "label": "poster",
          "constraint": {},
          "thumbnails": []
        }
      },
      "overview": {
        "type": "StructuredText",
        "config": {
          "label": "Overview",
          "placeholder": "",
          "allowTargetBlank": false,
          "multi": "paragraph"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "project_details": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
